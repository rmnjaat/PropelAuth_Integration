{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python FastAPI Debugger",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "main:app",
                "--reload",
                "--host",
                "0.0.0.0",
                "--port",
                "8000"
            ],
            "jinja": true,
            "justMyCode": true,
            "cwd": "${workspaceFolder}",
            "env": {},
            "console": "integratedTerminal"
        }
    ]
}